{
	"title": "SeventySix API Endpoints",
	"uid": "seventysix-api-endpoints",
	"tags": ["api", "seventysix"],
	"timezone": "browser",
	"refresh": "5s",
	"time": {
		"from": "now-15m",
		"to": "now"
	},
	"panels": [
		{
			"id": 1,
			"title": "Request Rate per Endpoint",
			"type": "timeseries",
			"datasource": {
				"type": "prometheus",
				"uid": "PBFA97CFB590B2093"
			},
			"gridPos": {
				"h": 9,
				"w": 12,
				"x": 0,
				"y": 0
			},
			"targets": [
				{
					"expr": "sum by(http_route) (rate(http_server_request_duration_seconds_count{job=\"seventysix-api\"}[1m]))",
					"legendFormat": "{{http_route}}",
					"refId": "A"
				}
			],
			"fieldConfig": {
				"defaults": {
					"color": {
						"mode": "palette-classic"
					},
					"custom": {
						"drawStyle": "line",
						"lineInterpolation": "smooth",
						"lineWidth": 2,
						"fillOpacity": 20,
						"gradientMode": "opacity",
						"spanNulls": false,
						"showPoints": "never",
						"pointSize": 5,
						"stacking": {
							"mode": "none",
							"group": "A"
						},
						"axisPlacement": "auto",
						"axisLabel": "Requests/sec",
						"scaleDistribution": {
							"type": "linear"
						}
					},
					"unit": "reqps",
					"min": 0
				}
			},
			"options": {
				"tooltip": {
					"mode": "multi",
					"sort": "desc"
				},
				"legend": {
					"displayMode": "table",
					"placement": "bottom",
					"showLegend": true,
					"calcs": ["mean", "lastNotNull", "max"]
				}
			}
		},
		{
			"id": 2,
			"title": "Response Time Distribution (p50, p95, p99)",
			"type": "timeseries",
			"datasource": {
				"type": "prometheus",
				"uid": "PBFA97CFB590B2093"
			},
			"gridPos": {
				"h": 9,
				"w": 12,
				"x": 12,
				"y": 0
			},
			"targets": [
				{
					"expr": "histogram_quantile(0.50, sum by(le) (rate(http_server_request_duration_seconds_bucket{job=\"seventysix-api\"}[1m])))",
					"legendFormat": "p50 (median)",
					"refId": "A"
				},
				{
					"expr": "histogram_quantile(0.95, sum by(le) (rate(http_server_request_duration_seconds_bucket{job=\"seventysix-api\"}[1m])))",
					"legendFormat": "p95",
					"refId": "B"
				},
				{
					"expr": "histogram_quantile(0.99, sum by(le) (rate(http_server_request_duration_seconds_bucket{job=\"seventysix-api\"}[1m])))",
					"legendFormat": "p99",
					"refId": "C"
				}
			],
			"fieldConfig": {
				"defaults": {
					"color": {
						"mode": "palette-classic"
					},
					"custom": {
						"drawStyle": "line",
						"lineInterpolation": "smooth",
						"lineWidth": 3,
						"fillOpacity": 15,
						"gradientMode": "opacity",
						"spanNulls": false,
						"showPoints": "never",
						"pointSize": 5,
						"stacking": {
							"mode": "none",
							"group": "A"
						},
						"axisPlacement": "auto",
						"axisLabel": "Duration",
						"scaleDistribution": {
							"type": "linear"
						}
					},
					"unit": "ms",
					"min": 0
				},
				"overrides": [
					{
						"matcher": {
							"id": "byName",
							"options": "p50 (median)"
						},
						"properties": [
							{
								"id": "color",
								"value": {
									"mode": "fixed",
									"fixedColor": "green"
								}
							}
						]
					},
					{
						"matcher": {
							"id": "byName",
							"options": "p95"
						},
						"properties": [
							{
								"id": "color",
								"value": {
									"mode": "fixed",
									"fixedColor": "yellow"
								}
							}
						]
					},
					{
						"matcher": {
							"id": "byName",
							"options": "p99"
						},
						"properties": [
							{
								"id": "color",
								"value": {
									"mode": "fixed",
									"fixedColor": "red"
								}
							}
						]
					}
				]
			},
			"options": {
				"tooltip": {
					"mode": "multi",
					"sort": "none"
				},
				"legend": {
					"displayMode": "table",
					"placement": "bottom",
					"showLegend": true,
					"calcs": ["mean", "lastNotNull", "max"]
				}
			}
		},
		{
			"id": 3,
			"title": "HTTP Status Code Distribution",
			"type": "timeseries",
			"datasource": {
				"type": "prometheus",
				"uid": "PBFA97CFB590B2093"
			},
			"gridPos": {
				"h": 9,
				"w": 12,
				"x": 0,
				"y": 9
			},
			"targets": [
				{
					"expr": "sum by(http_response_status_code) (rate(http_server_request_duration_seconds_count{job=\"seventysix-api\"}[1m]))",
					"legendFormat": "{{http_response_status_code}}",
					"refId": "A"
				}
			],
			"fieldConfig": {
				"defaults": {
					"color": {
						"mode": "palette-classic"
					},
					"custom": {
						"drawStyle": "bars",
						"lineInterpolation": "linear",
						"barAlignment": 0,
						"lineWidth": 1,
						"fillOpacity": 80,
						"gradientMode": "none",
						"spanNulls": false,
						"showPoints": "never",
						"pointSize": 5,
						"stacking": {
							"mode": "normal",
							"group": "A"
						},
						"axisPlacement": "auto",
						"axisLabel": "Requests/sec",
						"scaleDistribution": {
							"type": "linear"
						}
					},
					"unit": "reqps",
					"min": 0
				},
				"overrides": [
					{
						"matcher": {
							"id": "byRegexp",
							"options": "^2..$"
						},
						"properties": [
							{
								"id": "color",
								"value": {
									"mode": "fixed",
									"fixedColor": "green"
								}
							}
						]
					},
					{
						"matcher": {
							"id": "byRegexp",
							"options": "^4..$"
						},
						"properties": [
							{
								"id": "color",
								"value": {
									"mode": "fixed",
									"fixedColor": "yellow"
								}
							}
						]
					},
					{
						"matcher": {
							"id": "byRegexp",
							"options": "^5..$"
						},
						"properties": [
							{
								"id": "color",
								"value": {
									"mode": "fixed",
									"fixedColor": "red"
								}
							}
						]
					}
				]
			},
			"options": {
				"tooltip": {
					"mode": "multi",
					"sort": "desc"
				},
				"legend": {
					"displayMode": "table",
					"placement": "bottom",
					"showLegend": true,
					"calcs": ["sum", "lastNotNull"]
				}
			}
		},
		{
			"id": 4,
			"title": "Total Requests (Current vs 1 Hour Ago)",
			"type": "stat",
			"datasource": {
				"type": "prometheus",
				"uid": "PBFA97CFB590B2093"
			},
			"gridPos": {
				"h": 9,
				"w": 12,
				"x": 12,
				"y": 9
			},
			"targets": [
				{
					"expr": "sum(increase(http_server_request_duration_seconds_count{job=\"seventysix-api\"}[5m]))",
					"legendFormat": "Last 5 minutes",
					"refId": "A"
				},
				{
					"expr": "sum(rate(http_server_request_duration_seconds_count{job=\"seventysix-api\"}[1m]))",
					"legendFormat": "Current rate/sec",
					"refId": "B"
				}
			],
			"fieldConfig": {
				"defaults": {
					"color": {
						"mode": "thresholds"
					},
					"mappings": [],
					"thresholds": {
						"mode": "absolute",
						"steps": [
							{
								"color": "red",
								"value": null
							},
							{
								"color": "yellow",
								"value": 1
							},
							{
								"color": "green",
								"value": 10
							}
						]
					},
					"unit": "short"
				},
				"overrides": [
					{
						"matcher": {
							"id": "byName",
							"options": "Current rate/sec"
						},
						"properties": [
							{
								"id": "unit",
								"value": "reqps"
							}
						]
					}
				]
			},
			"options": {
				"reduceOptions": {
					"values": false,
					"calcs": ["lastNotNull"]
				},
				"orientation": "auto",
				"textMode": "value_and_name",
				"colorMode": "background",
				"graphMode": "area",
				"justifyMode": "auto"
			}
		}
	],
	"schemaVersion": 38,
	"version": 2
}
