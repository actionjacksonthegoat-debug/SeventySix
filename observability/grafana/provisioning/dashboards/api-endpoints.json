{
	"title": "SeventySix API Endpoints",
	"uid": "seventysix-api-endpoints",
	"tags": ["api", "seventysix"],
	"timezone": "browser",
	"refresh": "30s",
	"time": {
		"from": "now-1h",
		"to": "now"
	},
	"panels": [
		{
			"id": 1,
			"title": "Total Requests (Current vs 1 Hour Ago)",
			"type": "stat",
			"datasource": {
				"type": "prometheus",
				"uid": "PBFA97CFB590B2093"
			},
			"gridPos": {
				"h": 8,
				"w": 8,
				"x": 0,
				"y": 0
			},
			"targets": [
				{
					"expr": "sum(increase(http_server_request_duration_seconds_count{job=\"seventysix-api\"}[5m]))",
					"legendFormat": "Last 5 min",
					"refId": "A"
				},
				{
					"expr": "sum(increase(http_server_request_duration_seconds_count{job=\"seventysix-api\"}[1h] offset 1h))",
					"legendFormat": "1 hour ago",
					"refId": "B"
				},
				{
					"expr": "sum(rate(http_server_request_duration_seconds_count{job=\"seventysix-api\"}[1m]))",
					"legendFormat": "Rate/sec",
					"refId": "C"
				}
			],
			"fieldConfig": {
				"defaults": {
					"color": {
						"mode": "thresholds"
					},
					"thresholds": {
						"mode": "absolute",
						"steps": [
							{
								"color": "blue",
								"value": null
							},
							{
								"color": "green",
								"value": 10
							},
							{
								"color": "yellow",
								"value": 100
							}
						]
					},
					"unit": "short"
				},
				"overrides": [
					{
						"matcher": {
							"id": "byName",
							"options": "Rate/sec"
						},
						"properties": [
							{
								"id": "unit",
								"value": "reqps"
							}
						]
					}
				]
			},
			"options": {
				"reduceOptions": {
					"values": false,
					"calcs": ["lastNotNull"]
				},
				"orientation": "auto",
				"textMode": "value_and_name",
				"colorMode": "background",
				"graphMode": "area",
				"justifyMode": "auto"
			}
		},
		{
			"id": 2,
			"title": "Top 10 Slowest Endpoints (p95)",
			"type": "bargauge",
			"datasource": {
				"type": "prometheus",
				"uid": "PBFA97CFB590B2093"
			},
			"gridPos": {
				"h": 8,
				"w": 16,
				"x": 8,
				"y": 0
			},
			"targets": [
				{
					"expr": "topk(10, histogram_quantile(0.95, sum by(le, http_route) (rate(http_server_request_duration_seconds_bucket{job=\"seventysix-api\"}[5m]))) * 1000)",
					"legendFormat": "{{http_route}}",
					"refId": "A"
				}
			],
			"fieldConfig": {
				"defaults": {
					"color": {
						"mode": "thresholds"
					},
					"thresholds": {
						"mode": "absolute",
						"steps": [
							{
								"color": "green",
								"value": null
							},
							{
								"color": "yellow",
								"value": 50
							},
							{
								"color": "orange",
								"value": 200
							},
							{
								"color": "red",
								"value": 500
							}
						]
					},
					"unit": "ms",
					"min": 0
				}
			},
			"options": {
				"reduceOptions": {
					"values": false,
					"calcs": ["lastNotNull"]
				},
				"orientation": "horizontal",
				"displayMode": "gradient",
				"showUnfilled": true
			}
		},
		{
			"id": 3,
			"title": "HTTP Status Codes Over Time",
			"type": "timeseries",
			"datasource": {
				"type": "prometheus",
				"uid": "PBFA97CFB590B2093"
			},
			"gridPos": {
				"h": 8,
				"w": 12,
				"x": 0,
				"y": 8
			},
			"targets": [
				{
					"expr": "sum by(http_response_status_code) (rate(http_server_request_duration_seconds_count{job=\"seventysix-api\"}[1m]))",
					"legendFormat": "HTTP {{http_response_status_code}}",
					"refId": "A"
				}
			],
			"fieldConfig": {
				"defaults": {
					"color": {
						"mode": "palette-classic"
					},
					"custom": {
						"drawStyle": "bars",
						"lineInterpolation": "linear",
						"barAlignment": 0,
						"lineWidth": 1,
						"fillOpacity": 80,
						"gradientMode": "none",
						"showPoints": "never",
						"stacking": {
							"mode": "normal"
						},
						"axisPlacement": "auto",
						"axisLabel": "req/sec"
					},
					"unit": "reqps",
					"min": 0
				},
				"overrides": [
					{
						"matcher": {
							"id": "byRegexp",
							"options": ".*2[0-9]{2}.*"
						},
						"properties": [
							{
								"id": "color",
								"value": {
									"mode": "fixed",
									"fixedColor": "green"
								}
							}
						]
					},
					{
						"matcher": {
							"id": "byRegexp",
							"options": ".*4[0-9]{2}.*"
						},
						"properties": [
							{
								"id": "color",
								"value": {
									"mode": "fixed",
									"fixedColor": "yellow"
								}
							}
						]
					},
					{
						"matcher": {
							"id": "byRegexp",
							"options": ".*5[0-9]{2}.*"
						},
						"properties": [
							{
								"id": "color",
								"value": {
									"mode": "fixed",
									"fixedColor": "red"
								}
							}
						]
					}
				]
			},
			"options": {
				"tooltip": {
					"mode": "multi",
					"sort": "desc"
				},
				"legend": {
					"displayMode": "table",
					"placement": "right",
					"showLegend": true,
					"calcs": ["sum", "mean", "lastNotNull"]
				}
			}
		},
		{
			"id": 4,
			"title": "Error Rate (4xx & 5xx)",
			"type": "timeseries",
			"datasource": {
				"type": "prometheus",
				"uid": "PBFA97CFB590B2093"
			},
			"gridPos": {
				"h": 8,
				"w": 12,
				"x": 12,
				"y": 8
			},
			"targets": [
				{
					"expr": "sum(rate(http_server_request_duration_seconds_count{job=\"seventysix-api\",http_response_status_code=~\"4..\"}[1m]))",
					"legendFormat": "4xx Client Errors",
					"refId": "A"
				},
				{
					"expr": "sum(rate(http_server_request_duration_seconds_count{job=\"seventysix-api\",http_response_status_code=~\"5..\"}[1m]))",
					"legendFormat": "5xx Server Errors",
					"refId": "B"
				},
				{
					"expr": "(sum(rate(http_server_request_duration_seconds_count{job=\"seventysix-api\",http_response_status_code=~\"[45]..\"}[1m])) / sum(rate(http_server_request_duration_seconds_count{job=\"seventysix-api\"}[1m]))) * 100",
					"legendFormat": "Error Rate %",
					"refId": "C"
