{
	"title": "SeventySix System Overview",
	"uid": "seventysix-system-overview",
	"tags": ["overview", "system", "seventysix"],
	"timezone": "browser",
	"refresh": "5s",
	"time": {
		"from": "now-30m",
		"to": "now"
	},
	"panels": [
		{
			"id": 1,
			"title": "API Health Status",
			"type": "stat",
			"datasource": {
				"type": "prometheus",
				"uid": "PBFA97CFB590B2093"
			},
			"gridPos": {
				"h": 5,
				"w": 6,
				"x": 0,
				"y": 0
			},
			"targets": [
				{
					"expr": "up{job=\"seventysix-api\"}",
					"refId": "A"
				}
			],
			"fieldConfig": {
				"defaults": {
					"mappings": [
						{
							"type": "value",
							"options": {
								"0": {
									"text": "DOWN",
									"color": "red"
								},
								"1": {
									"text": "UP",
									"color": "green"
								}
							}
						}
					],
					"thresholds": {
						"mode": "absolute",
						"steps": [
							{
								"color": "red",
								"value": null
							},
							{
								"color": "green",
								"value": 1
							}
						]
					},
					"unit": "none"
				}
			},
			"options": {
				"reduceOptions": {
					"values": false,
					"calcs": ["lastNotNull"]
				},
				"orientation": "auto",
				"textMode": "value",
				"colorMode": "background",
				"graphMode": "none",
				"justifyMode": "center"
			}
		},
		{
			"id": 2,
			"title": "Total Requests (Last Hour)",
			"type": "stat",
			"datasource": {
				"type": "prometheus",
				"uid": "PBFA97CFB590B2093"
			},
			"gridPos": {
				"h": 5,
				"w": 6,
				"x": 6,
				"y": 0
			},
			"targets": [
				{
					"expr": "sum(increase(http_server_request_duration_seconds_count{job=\"seventysix-api\"}[1h]))",
					"refId": "A"
				}
			],
			"fieldConfig": {
				"defaults": {
					"color": {
						"mode": "thresholds"
					},
					"thresholds": {
						"mode": "absolute",
						"steps": [
							{
								"color": "blue",
								"value": null
							},
							{
								"color": "green",
								"value": 100
							},
							{
								"color": "yellow",
								"value": 1000
							}
						]
					},
					"unit": "short",
					"decimals": 0
				}
			},
			"options": {
				"reduceOptions": {
					"values": false,
					"calcs": ["lastNotNull"]
				},
				"orientation": "auto",
				"textMode": "value_and_name",
				"colorMode": "background",
				"graphMode": "area",
				"justifyMode": "center"
			}
		},
		{
			"id": 3,
			"title": "Error Rate (%)",
			"type": "stat",
			"datasource": {
				"type": "prometheus",
				"uid": "PBFA97CFB590B2093"
			},
			"gridPos": {
				"h": 5,
				"w": 6,
				"x": 12,
				"y": 0
			},
			"targets": [
				{
					"expr": "100 * (sum(rate(http_server_request_duration_seconds_count{job=\"seventysix-api\",http_response_status_code=~\"5..\"}[5m])) / sum(rate(http_server_request_duration_seconds_count{job=\"seventysix-api\"}[5m])))",
					"refId": "A"
				}
			],
			"fieldConfig": {
				"defaults": {
					"color": {
						"mode": "thresholds"
					},
					"thresholds": {
						"mode": "absolute",
						"steps": [
							{
								"color": "green",
								"value": null
							},
							{
								"color": "yellow",
								"value": 1
							},
							{
								"color": "orange",
								"value": 5
							},
							{
								"color": "red",
								"value": 10
							}
						]
					},
					"unit": "percent",
					"decimals": 2,
					"min": 0,
					"max": 100
				}
			},
			"options": {
				"reduceOptions": {
					"values": false,
					"calcs": ["lastNotNull"]
				},
				"orientation": "auto",
				"textMode": "value_and_name",
				"colorMode": "background",
				"graphMode": "area",
				"justifyMode": "center"
			}
		},
		{
			"id": 4,
			"title": "Avg Response Time (p95)",
			"type": "stat",
			"datasource": {
				"type": "prometheus",
				"uid": "PBFA97CFB590B2093"
			},
			"gridPos": {
				"h": 5,
				"w": 6,
				"x": 18,
				"y": 0
			},
			"targets": [
				{
					"expr": "histogram_quantile(0.95, sum by(le) (rate(http_server_request_duration_seconds_bucket{job=\"seventysix-api\"}[5m]))) * 1000",
					"refId": "A"
				}
			],
			"fieldConfig": {
				"defaults": {
					"color": {
						"mode": "thresholds"
					},
					"thresholds": {
						"mode": "absolute",
						"steps": [
							{
								"color": "green",
								"value": null
							},
							{
								"color": "yellow",
								"value": 200
							},
							{
								"color": "orange",
								"value": 500
							},
							{
								"color": "red",
								"value": 1000
							}
						]
					},
					"unit": "ms",
					"decimals": 1
				}
			},
			"options": {
				"reduceOptions": {
					"values": false,
					"calcs": ["lastNotNull"]
				},
				"orientation": "auto",
				"textMode": "value_and_name",
				"colorMode": "background",
				"graphMode": "area",
				"justifyMode": "center"
			}
		},
		{
			"id": 9,
			"title": "Memory Usage (Process)",
			"type": "timeseries",
			"datasource": {
				"type": "prometheus",
				"uid": "PBFA97CFB590B2093"
			},
			"gridPos": {
				"h": 7,
				"w": 12,
				"x": 0,
				"y": 21
			},
			"targets": [
				{
					"expr": "dotnet_process_memory_working_set_bytes{job=\"seventysix-api\"} / 1024 / 1024",
					"legendFormat": "Working Set - {{environment}}",
					"refId": "A"
				}
			],
			"fieldConfig": {
				"defaults": {
					"color": {
						"mode": "palette-classic"
					},
					"custom": {
						"drawStyle": "line",
						"lineInterpolation": "smooth",
						"lineWidth": 2,
						"fillOpacity": 20,
						"gradientMode": "opacity",
						"showPoints": "never",
						"stacking": {
							"mode": "none"
						},
						"axisPlacement": "auto",
						"axisLabel": "Memory"
					},
					"unit": "decmbytes",
					"min": 0
				}
			},
			"options": {
				"tooltip": {
					"mode": "multi",
					"sort": "none"
				},
				"legend": {
					"displayMode": "list",
					"placement": "bottom",
					"showLegend": true,
					"calcs": ["mean", "max", "lastNotNull"]
				}
			}
		},
		{
			"id": 10,
			"title": "GC Collections by Generation",
			"type": "timeseries",
			"datasource": {
				"type": "prometheus",
				"uid": "PBFA97CFB590B2093"
			},
			"gridPos": {
				"h": 7,
				"w": 12,
				"x": 12,
				"y": 21
			},
			"targets": [
				{
					"expr": "rate(dotnet_gc_collections_total{job=\"seventysix-api\",environment=\"local\"}[1m])",
					"legendFormat": "{{gc_heap_generation}} Collections/sec",
					"refId": "A"
				}
			],
			"fieldConfig": {
				"defaults": {
					"color": {
						"mode": "palette-classic"
					},
					"custom": {
						"drawStyle": "line",
						"lineInterpolation": "smooth",
						"lineWidth": 2,
						"fillOpacity": 15,
						"gradientMode": "opacity",
						"showPoints": "never",
						"stacking": {
							"mode": "none"
						},
						"axisPlacement": "auto"
					},
					"min": 0
				},
				"overrides": [
					{
						"matcher": {
							"id": "byRegexp",
							"options": ".*GC.*"
						},
						"properties": [
							{
								"id": "unit",
								"value": "ops"
							},
							{
								"id": "custom.axisPlacement",
								"value": "left"
							}
						]
					},
					{
						"matcher": {
							"id": "byRegexp",
							"options": ".*CPU.*"
						},
						"properties": [
							{
								"id": "unit",
								"value": "percent"
							},
							{
								"id": "custom.axisPlacement",
								"value": "right"
							}
						]
					}
				]
			},
			"options": {
				"tooltip": {
					"mode": "multi",
					"sort": "none"
				},
				"legend": {
					"displayMode": "list",
					"placement": "bottom",
					"showLegend": true,
					"calcs": ["mean", "max"]
				}
			}
		},
		{
			"id": 11,
			"title": "CPU Usage by Mode",
			"type": "timeseries",
			"datasource": {
				"type": "prometheus",
				"uid": "PBFA97CFB590B2093"
			},
			"gridPos": {
				"h": 7,
				"w": 12,
				"x": 0,
				"y": 28
			},
			"targets": [
				{
					"expr": "rate(dotnet_process_cpu_time_seconds_total{job=\"seventysix-api\",environment=\"local\"}[1m]) * 100",
					"legendFormat": "{{cpu_mode}} CPU %",
					"refId": "A"
				}
			],
			"fieldConfig": {
				"defaults": {
					"color": {
						"mode": "palette-classic"
					},
					"custom": {
						"drawStyle": "line",
						"lineInterpolation": "smooth",
						"lineWidth": 2,
						"fillOpacity": 20,
						"gradientMode": "opacity",
						"showPoints": "never",
						"stacking": {
							"mode": "normal"
						},
						"axisPlacement": "auto"
					},
					"unit": "percent",
					"min": 0,
					"max": 100
				}
			},
			"options": {
				"tooltip": {
					"mode": "multi",
					"sort": "desc"
				},
				"legend": {
					"displayMode": "list",
					"placement": "bottom",
					"showLegend": true,
					"calcs": ["mean", "max", "lastNotNull"]
				}
			}
		},
		{
			"id": 12,
			"title": "Thread Pool Queue Length",
			"type": "timeseries",
			"datasource": {
				"type": "prometheus",
				"uid": "PBFA97CFB590B2093"
			},
			"gridPos": {
				"h": 7,
				"w": 12,
				"x": 12,
				"y": 28
			},
			"targets": [
				{
					"expr": "dotnet_thread_pool_queue_length_total{job=\"seventysix-api\",environment=\"local\"}",
					"legendFormat": "Queue Length",
					"refId": "A"
				}
			],
			"fieldConfig": {
				"defaults": {
					"color": {
						"mode": "palette-classic"
					},
					"custom": {
						"drawStyle": "line",
						"lineInterpolation": "smooth",
						"lineWidth": 2,
						"fillOpacity": 10,
						"gradientMode": "none",
						"showPoints": "auto",
						"stacking": {
							"mode": "none"
						},
						"axisPlacement": "auto"
					},
					"unit": "short",
					"min": 0
				}
			},
			"options": {
				"tooltip": {
					"mode": "single",
					"sort": "none"
				},
				"legend": {
					"displayMode": "list",
					"placement": "bottom",
					"showLegend": true,
					"calcs": ["mean", "max", "lastNotNull"]
				}
			}
		}
	],
	"schemaVersion": 38,
	"version": 1
}
