{
	"Logging": {
		"LogLevel": {
			"Default": "Debug",
			"Microsoft.AspNetCore": "Information",
			"Microsoft.EntityFrameworkCore.Database.Command": "Information",
			"SeventySix": "Debug"
		}
	},
	"Testing": {
		"RunIntegrationTests": false
	},
	"Cache": {
		"OutputCache": {
			"Policies": {
				"Weather": {
					"DurationSeconds": 10,
					"Enabled": false
				},
				"Users": {
					"DurationSeconds": 5,
					"Enabled": false
				},
				"Logs": {
					"DurationSeconds": 5,
					"Enabled": false
				},
				"LogCharts": {
					"DurationSeconds": 5,
					"Enabled": false
				},
				"Health": {
					"DurationSeconds": 5,
					"Enabled": false
				},
				"ThirdPartyRequests": {
					"DurationSeconds": 5,
					"Enabled": false
				}
			}
		}
	}
}
