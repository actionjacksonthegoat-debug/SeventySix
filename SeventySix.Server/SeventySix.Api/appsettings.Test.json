{
	"ConnectionStrings": {
		"DefaultConnection": "Host=localhost;Port=5432;Database=seventysix_test;Username=postgres;Password=TestPassword;Pooling=true;Include Error Detail=true"
	},
	"AdminSeeder": {
		"Enabled": false
	},
	"BackgroundJobs": {
		"Enabled": false
	},
	"OpenTelemetry": {
		"Enabled": false,
		"OtlpEndpoint": "http://localhost:4317"
	},
	"HealthChecks": {
		"Enabled": false
	},
	"ResponseCompression": {
		"Enabled": false
	},
	"Logging": {
		"LogLevel": {
			"Default": "Warning",
			"Microsoft": "Error",
			"Microsoft.AspNetCore": "Error",
			"Microsoft.EntityFrameworkCore": "Error",
			"Microsoft.EntityFrameworkCore.Database.Command": "Error",
			"Microsoft.Hosting.Lifetime": "Error",
			"SeventySix": "Warning",
			"System": "Error",
			"Wolverine": "Error",
			"JasperFx": "Error",
			"Serilog": "Error"
		}
	},
	"Security": {
		"EnforceHttps": false,
		"AllowHttpForHealthChecks": true,
		"AllowHttpForMetrics": true,
		"AllowHttpForOpenApi": true
	},
	"Testing": {
		"RunIntegrationTests": false
	},
	"RateLimiting": {
		"Enabled": true,
		"PermitLimit": 250,
		"WindowSeconds": 3600
	},
	"Auth": {
		"RateLimit": {
			"LoginAttemptsPerMinute": 1000,
			"RegisterAttemptsPerHour": 1000,
			"TokenRefreshPerMinute": 1000
		}
	},
	"Cache": {
		"OutputCache": {
			"Policies": {
				"users": {
					"DurationSeconds": 5,
					"Enabled": false
				},
				"logs": {
					"DurationSeconds": 5,
					"Enabled": false
				},
				"health": {
					"DurationSeconds": 5,
					"Enabled": false
				},
				"thirdpartyrequests": {
					"DurationSeconds": 5,
					"Enabled": false
				}
			}
		}
	},
	"ThirdPartyApiLimits": {
		"DefaultDailyLimit": 1000,
		"Enabled": false,
		"Limits": {
			"BrevoEmail": {
				"DailyLimit": 250,
				"Enabled": false
			}
		}
	}
}
