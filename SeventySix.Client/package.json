{
  "name": "seventy-six.client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "prepare": "playwright install chromium",
    "start": "ng serve",
    "build": "ng build",
    "test": "node scripts/architecture-tests.mjs && vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "eslint . --fix && dprint fmt && eslint . --fix",
    "format:check": "eslint .",
    "test:e2e": "node scripts/e2e-full.mjs",
    "test:e2e:keepalive": "node scripts/e2e-full.mjs --keepalive",
    "generate:openapi": "node scripts/generate-openapi.mjs",
    "generate:openapi:types-only": "openapi-typescript ../openapi.json -o src/app/shared/generated-open-api/generated-open-api.ts",
    "generate:icons": "node scripts/generate-icons.mjs",
    "update:deps": "npx npm-check-updates -u --reject eslint && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.5",
    "@angular/cdk": "^21.1.5",
    "@angular/common": "^21.1.5",
    "@angular/compiler": "^21.1.5",
    "@angular/core": "^21.1.5",
    "@angular/forms": "^21.1.5",
    "@angular/material": "^21.1.5",
    "@angular/platform-browser": "^21.1.5",
    "@angular/router": "^21.1.5",
    "@angular/service-worker": "^21.1.5",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.212.0",
    "@opentelemetry/instrumentation": "^0.212.0",
    "@opentelemetry/instrumentation-document-load": "^0.57.0",
    "@opentelemetry/instrumentation-fetch": "^0.212.0",
    "@opentelemetry/resources": "^2.5.1",
    "@opentelemetry/sdk-trace-web": "^2.5.1",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@tanstack/angular-query-experimental": "^5.90.25",
    "altcha": "^2.3.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dompurify": "^3.3.1",
    "ngx-skeleton-loader": "^12.0.0",
    "qrcode": "^1.5.4",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.2.3",
    "@analogjs/vitest-angular": "^2.2.3",
    "@angular/build": "^21.1.5",
    "@angular/cli": "^21.1.5",
    "@angular/compiler-cli": "^21.1.5",
    "@angular/pwa": "^21.1.4",
    "@axe-core/playwright": "^4.11.1",
    "@playwright/test": "^1.58.2",
    "@stylistic/eslint-plugin": "^5.9.0",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^25.3.0",
    "@types/qrcode": "^1.5.6",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.1",
    "eslint-plugin-playwright": "^2.7.0",
    "happy-dom": "20.7.0",
    "jiti": "latest",
    "openapi-typescript": "^7.13.0",
    "otpauth": "^9.5.0",
    "playwright": "^1.58.2",
    "sharp": "^0.34.5",
    "typescript": "~5.9.3",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "qs": "6.15.0",
    "hono": "4.12.2",
    "tar": "7.5.9"
  }
